{
  "name": "Wavetronix Tab",
  "description": "Wavetronix tab",
  "version": "1.2.6",
  "manifest_version": 2,
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
         "storage"
     ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
